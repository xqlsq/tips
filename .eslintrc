{
    "env": {
      "node": true,
       "es6": true
    },
    "rules": {
        "no-undef": 2,
        "semi": 2,
        "strict": 0,
        "quotes": 0,
        "indent": ["error", "tab"],
        "curly": [2, "multi-line"],
        "no-use-before-define": [2, "nofunc"],
        "no-unused-vars": [2, "all"],
        "no-mixed-requires": [2, true],
        "max-depth": [2, 5],
        "max-len": [2, 80, 4],
        "max-params": [2, 6],
        "max-statements": [2, 20],
        "eqeqeq": 0,
        "new-cap": 0,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-lonely-if": 2,
        "no-path-concat": 0,
        "comma-dangle": 0,
        "complexity": [2, 20],
        "no-floating-decimal": 2,
        "no-void": 2,
        "no-sync": 2,
        "consistent-this": [2, "nope-dont-capture-this"],
        "max-nested-callbacks": [2, 3],
        "no-nested-ternary": 2,
        "keyword-spacing": 2,
        "space-before-function-paren": [2, "never"],
        "spaced-comment": [2, "always"],
    }
}
